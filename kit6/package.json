{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A React starter kit",
  "main": "src/main.jsx",
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --inline --hot --devtool eval --progress --colors --content-base build",
    "lint": "eslint 'src/**/*.@(js|jsx)'",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "validate": "npm ls"
  },
  "keywords": [
    "es6",
    "react",
    "babel6",
    "webpack",
    "boilerplate"
  ],
  "author": "soulmachine",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-runtime": "^6.3.13",
    "history": "^1.13.1",
    "material-ui": "^0.13.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.2",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "css-loader": "^0.23.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.0.0",
    "eslint-plugin-react": "^3.11.3",
    "file-loader": "^0.8.5",
    "jsdom": "^7.2.1",
    "mocha": "^2.3.4",
    "open-browser-webpack-plugin": "0.0.1",
    "precommit-hook": "^3.0.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
